{
  "name": "@mintpass/challenge",
  "version": "1.1.3",
  "description": "MintPass plebbit-js challenge implementation",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "test": "yarn build && ./scripts/test-with-node.sh",
    "test:manual": "yarn build && cd ../contracts && npx hardhat test ../challenge/test/mintpass-integration.test.js --network localhost",
    "clean": "rm -rf dist/"
  },
  "dependencies": {
    "@keyv/sqlite": "^4.0.5",
    "env-paths": "^3.0.0",
    "keyv": "5.5.0",
    "multiformats": "^13.1.0",
    "uint8arrays": "^4.0.3",
    "viem": "2.0.0"
  },
  "peerDependencies": {
    "@plebbit/plebbit-js": ">=0.0.0"
  },
  "devDependencies": {
    "@plebbit/plebbit-js": "https://github.com/plebbit/plebbit-js.git#d29c607ca27200467101e64e637755565a5d6c14",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.7.8",
    "chai": "^4.2.0",
    "ethers": "6.4.0",
    "hardhat": "^2.19.0",
    "kubo": "0.36.0",
    "tempy": "^3.1.0",
    "typescript": "^5.6.3"
  },
  "keywords": [
    "plebbit",
    "challenge",
    "nft",
    "mintpass",
    "authentication"
  ],
  "author": "Plebbit Labs",
  "license": "MIT"
}
